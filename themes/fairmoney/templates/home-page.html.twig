<div id="pitch" class="background-gradient">
	<div class="container">
		<div class="row">
			<div id="why-fair-money-pitch" class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pitch-wrap">
				<h1>{{ node.field_pitch_title['und'][0]['value'] }}</h1> <!-- CMS CONTENT HERE -->
				<ul class="pitch-list">
			        <li><img src="/sites/all/themes/fairmoney/images/tick.png" alt="" class="homepage-bullet">{{ node.field_pitch_item['und'][0]['value'] }}</li> <!-- CMS CONTENT HERE -->
			        <li><img src="/sites/all/themes/fairmoney/images/tick.png" alt="" class="homepage-bullet">{{ node.field_pitch_item['und'][1]['value'] }}</li> <!-- CMS CONTENT HERE -->
			        <li><img src="/sites/all/themes/fairmoney/images/tick.png" alt="" class="homepage-bullet">{{ node.field_pitch_item['und'][2]['value'] }}</li> <!-- CMS CONTENT HERE -->
			        <li><img src="/sites/all/themes/fairmoney/images/tick.png" alt="" class="homepage-bullet">{{ node.field_pitch_item['und'][3]['value'] }}</li> <!-- CMS CONTENT HERE -->
			      </ul>
			</div>
			<div id="what-our-clients-say" class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pitch-wrap">
				<h1>{{ node.field_quote_title['und'][0]['value'] }}</h1> <!-- CMS CONTENT HERE -->
				<blockquote>
					{{ node.field_quote_quote_text['und'][0]['value'] }} <!-- CMS CONTENT HERE -->
					 <footer>{{ node.field_quote_author['und'][0]['value'] }}</footer> <!-- CMS CONTENT HERE -->
				</blockquote>
			</div>
		</div>
	</div>
</div>
<div id="sample-representative-loans" class="background-plain">
  <div class="drop-arrow-decoration-white">
    <div class="white-arrow-bg white-arrow-bg-left"></div>
    <img src="/sites/all/themes/fairmoney/images/fairmoney-icon-down-white.png"/>
    <div class="white-arrow-bg white-arrow-bg-right"></div>
  </div>
	<div class="container">
		<div class="row">
			<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 srl-wrap">
				<h1>{{ node.field_srl_title['und'][0]['value'] }}</h1>
				<small>{{ node.field_srl_description['und'][0]['value'] }}</small>
			</div>
		</div>
		<div class="row credit-wrap">
			<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 credit">
				<h3 class="good-credit">Good Credit <i id="top-loan-gold-hover" class="fa fa-question-circle"></i></h3>
				<small>{{ node.field_srl_good_credit['und'][0]['value'] }}</small>
			</div>
			<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 credit">
				<h3 class="poor-credit">Poor Credit <i id="top-loan-silver-hover" class="fa fa-question-circle"></i></h3>
				<small>{{ node.field_srl_poor_credit['und'][0]['value'] }}</small>
			</div>
			<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 credit">
				<h3 class="bad-credit">Bad Credit <i id="top-loan-bronze-hover" class="fa fa-question-circle"></i></h3>
				<small>{{ node.field_srl_bad_credit['und'][0]['value'] }}</small>
			</div>
			<div class="col-xs-12">
				<p class="info">{{ node.field_srl_information['und'][0]['value'] }}</p>
			</div>
		</div>
	</div>
</div>
