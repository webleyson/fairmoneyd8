					<div class="row top-content">
						<div class="col-xs-12">
							<div class="content-wrap dotted-bottom">
								<h1>{{ node.field_banner_title_1['und'][0]['value'] }}</h1> <!-- CMS CONTENT HERE -->
								<small>{{ node.field_banner_description_1['und'][0]['value'] }}</small> <!-- CMS CONTENT HERE -->
